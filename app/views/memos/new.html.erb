<div class="contents row">
  <div class="container">
    <h3>MEMO</h3>
    <%= form_with model: @memo, url: memos_path, local: true do |form| %>
      <%# <%= form.text_field :name, placeholder: "Nickname" %>
      <%= form.text_area :memo_text, placeholder: "text", rows: "10" %>
      <%= form.file_field :image, id: "image" %>
      <%= form.submit "SEND" %>
    <% end %>
  </div>
</div>